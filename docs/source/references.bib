doi = {10.1175/JPO-D-11-039.1},
file = {:Users/wpringle/Documents/References/{\%}5B15200485{\%}20-{\%}20Journal{\%}20of{\%}20Physical{\%}20Oceanography{\%}5D{\%}20Energetics{\%}20of{\%}20Barotropic{\%}20and{\%}20Baroclinic{\%}20Tides{\%}20in{\%}20the{\%}20Monterey{\%}20Bay{\%}20Area.pdf:pdf},
issn = {00223670},
journal = {Journal of Physical Oceanography},
keywords = {Baroclinic flows,Barotropic flows,Internal waves,Nonhydrostatic models,Numerical analysis/modeling,Ocean models},
number = {2},
pages = {272--290},
title = {{Energetics of barotropic and baroclinic tides in the Monterey Bay area}},
volume = {42},
year = {2012}
}
