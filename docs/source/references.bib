issn = {1364503X},
journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {Atmospheric modelling,Complex terrain,Large eddy simulation,Rotor modelling,Wind farm wakes,Wind turbine wakes},
