@article{Vinogradova2015,
abstract = {AbstractThe oceanic response to surface loading, such as that related to atmospheric pressure, freshwater exchange, and changes in the gravity field, is essential to our understanding of sea level variability. In particular, so-called self-attraction and loading (SAL) effects caused by the redistribution of mass within the land–atmosphere–ocean system can have a measurable impact on sea level. In this study, the nature of SAL-induced variability in sea level is examined in terms of its equilibrium (static) and nonequilibrium (dynamic) components, using a general circulation model that implicitly includes the physics of SAL. The additional SAL forcing is derived by decomposing ocean mass anomalies into spherical harmonics and then applying Love numbers to infer associated crustal displacements and gravitational shifts. This implementation of SAL physics incurs only a relatively small computational cost. Effects of SAL on sea level amount to about 10{\%} of the applied surface loading on average but depend str...},
