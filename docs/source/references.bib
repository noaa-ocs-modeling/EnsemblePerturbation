journal = {Climate Dynamics},
keywords = {Climate extremes,Dynamical downscaling,Ensemble,Global climate models,Regional climate models,Statistical evaluation},
number = {3-4},
pages = {863--884},
publisher = {Springer Berlin Heidelberg},
title = {{Evaluations of high-resolution dynamically downscaled ensembles over the contiguous United States}},
volume = {50},
year = {2018}
}
