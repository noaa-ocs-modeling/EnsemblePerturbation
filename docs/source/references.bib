issn = {08948755},
journal = {Journal of Climate},
keywords = {Circulation/Dynamics,Climate models,Lake effects,Mass fluxes/transport,Mesoscale models,Models and modeling,Physical meteorology and climatology,Regional models,Stationary waves},
