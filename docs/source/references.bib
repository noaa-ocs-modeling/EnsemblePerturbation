keywords = {Hurricane conditions,Inlet dynamics,Numerical modeling,Wave dynamics,Wave–current interactions},
number = {August 2017},
pages = {124--144},
publisher = {Elsevier},
title = {{Wave–current dynamics and interactions near the two inlets of a shallow lagoon–inlet–coastal ocean system under hurricane conditions}},
