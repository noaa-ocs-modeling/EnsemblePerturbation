pages = {244--261},
publisher = {Elsevier Ltd},
title = {{Fully coupled aero-hydrodynamic analysis of a semi-submersible FOWT using a dynamic fluid body interaction approach}},
url = {http://dx.doi.org/10.1016/j.renene.2016.02.021},
volume = {92},
