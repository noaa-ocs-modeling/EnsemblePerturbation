journal = {Ocean Dynamics},
keywords = {Coupled modeling,Hurricane,Ocean dynamics},
number = {11},
pages = {1535--1554},
title = {{Investigation of hurricane Ivan using the coupled ocean–atmosphere–wave–sediment transport (COAWST) model}},
volume = {64},
