issn = {03783839},
journal = {Coastal Engineering},
keywords = {CFD,OpenFOAM,Porous media,RANS,Two phase flow,Wave-structure interaction},
pages = {259--270},
publisher = {Elsevier B.V.},
title = {{Three-dimensional interaction of waves and porous coastal structures using OpenFOAM{\textregistered}. Part II: Application}},
