issn = {2077-1312},
journal = {Journal of Marine Science and Engineering},
keywords = {ADCIRC,Battery,EnKF,New York City,WRF,coastal flooding,ensemble,hurricane Sandy,predictability,storm surge},
month = {jun},
number = {2},
pages = {428--443},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Exploring Water Level Sensitivity for Metropolitan New York during Sandy (2012) Using Ensemble Storm Surge Simulations}},
