pages = {1640011},
title = {{Improvement of Tsunami Countermeasures Based on Lessons from The 2011 Great East Japan Earthquake and Tsunami — Situation After Five Years}},
url = {http://www.worldscientific.com/doi/abs/10.1142/S0578563416400118},
volume = {58},
year = {2016}
}
