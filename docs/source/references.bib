isbn = {1364-503X},
issn = {1364503X},
journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {Anisotropy,Gyre,Mesh adaptivity,Ocean,Western boundary current},
number = {1907},
