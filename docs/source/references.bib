issn = {1364-5021},
journal = {Proceedings of the Royal Society A: Mathematical, Physical and Engineering Sciences},
month = {nov},
number = {2172},
pages = {20140575--20140575},
title = {{Can small islands protect nearby coasts from tsunamis? An active experimental design approach}},
url = {http://rspa.royalsocietypublishing.org/content/470/2172/20140575},
volume = {470},
year = {2014}
}
