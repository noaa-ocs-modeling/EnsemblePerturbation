publisher = {Oceanography Society},
title = {{Earthquake and Tsunami Potential of the Hikurangi Subduction Thrust, New Zealand: Insights from Paleoseismology, GPS, and Tsunami Modeling}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84903177624{\&}partnerID=tZOtx3y1},
