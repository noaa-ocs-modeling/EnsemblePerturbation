issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {Coastal flood risk,DEMs,Neural network,SRTM},
number = {December 2017},
pages = {231--239},
publisher = {Elsevier},
title = {{CoastalDEM: A global coastal digital elevation model improved from SRTM using a neural network}},
url = {https://doi.org/10.1016/j.rse.2017.12.026},
volume = {206},
year = {2018}
