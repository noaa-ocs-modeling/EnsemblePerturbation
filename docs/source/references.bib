doi = {10.1080/10618560601046846},
file = {:Users/wpringle/Library/Application Support/Mendeley Desktop/Downloaded/Hagen, Zundel, Kojima - 2006 - Automatic, unstructured mesh generation for tidal calculations in a large domain.pdf:pdf},
