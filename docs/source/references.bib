journal = {Natural Hazards},
keywords = {Forecasting,Quantifying risk,Storm surge,Uncertainty},
number = {3},
pages = {1423--1449},
publisher = {Springer Netherlands},
title = {{Quantifying impacts of forecast uncertainties on predicted storm surges}},
volume = {88},
year = {2017}
}
