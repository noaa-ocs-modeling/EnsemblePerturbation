@article{Wei2016,
abstract = {{\textless}p{\textgreater}The South China Sea (SCS) and the Java Sea (JS) are connected through the Karimata Strait, Gaspar Strait, and the southern Natuna Sea, where the tides are often used as open boundary condition for tidal simulation in the SCS or Indonesian seas. Tides, tidal currents, and tidal energy fluxes of the principle constituents {\textless}i{\textgreater}K{\textless}/i{\textgreater}{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}, {\textless}i{\textgreater}O{\textless}/i{\textgreater}{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}, {\textless}i{\textgreater}Q{\textless}/i{\textgreater}{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}, {\textless}i{\textgreater}M{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}, {\textless}i{\textgreater}S{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}, and {\textless}i{\textgreater}N{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} at five stations in this area have been analyzed using in situ observational data. The results show that the diurnal tides are the dominant constituents in the entire study area. The constituent {\textless}i{\textgreater}K{\textless}/i{\textgreater}{\textless}sub{\textgreater}1{\textless}/sub{\textgreater} has the largest amplitude, exceeding 50 cm, whereas the amplitudes of {\textless}i{\textgreater}M{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} are smaller than 5 cm at all stations. The amplitudes of {\textless}i{\textgreater}S{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} may exceed {\textless}i{\textgreater}M{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} in the Karimata and Gaspar straits. Tidal currents are mostly of rectilinear type in this area. The semi-major axes lengths of the diurnal tidal current ellipses are about 10 cm s{\textless}sup{\textgreater}−1{\textless}/sup{\textgreater}, and those of the semidiurnal tidal currents are smaller than 5 cm s{\textless}sup{\textgreater}−1{\textless}/sup{\textgreater}. The diurnal tidal energy flows from the SCS to the JS. The semidiurnal tidal energy flows from the SCS to the JS through the Karimata Strait and the eastern part of the southern Natuna Sea but flows in the opposite direction in the Gaspar Strait and the western part of the southern Natuna Sea. Harmonic analysis of sea level and current observation also suggest that the study area is located in the antinodal band of the diurnal tidal waves, and in the nodal band of the semidiurnal tidal waves. Comparisons show that the existing models are basically consistent with the observational results, but further improvements are necessary.{\textless}/p{\textgreater}},
author = {Wei, Zexun and Fang, Guohong and Susanto, R. Dwi and Adi, Tukul Rameyo and Fan, Bin and Setiawan, Agus and Li, Shujiang and Wang, Yonggang and Gao, Xiumin},
doi = {10.5194/os-12-517-2016},
