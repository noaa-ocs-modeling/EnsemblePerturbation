volume = {26},
year = {2006}
}
@article{Higham1988,
abstract = {The nearest symmetric positive semidefinite matrix in the Frobenius norm to an arbitrary real matrix A is shown to be (B + H)/2, where H is the symmetric polar factor of B=(A + AT)/2. In the 2-norm a nearest symmetric positive semidefinite matrix, and its distance $\delta$2(A) from A, are given by a computationally challenging formula due to Halmos. We show how the bisection method can be applied to this formula to compute upper and lower bounds for $\delta$2(A) differing by no more than a given amount. A key ingredient is a stable and efficient test for positive definiteness, based on an attempted Choleski decomposition. For accurate computation of $\delta$2(A) we formulate the problem as one of zero finding and apply a hybrid Newton-bisection algorithm. Some numerical difficulties are discussed and illustrated by example.},
author = {Higham, Nicholas J.},
doi = {10.1016/0024-3795(88)90223-6},
file = {:Users/wpringle/Library/Application Support/Mendeley Desktop/Downloaded/Higham - 1988 - Computing a nearest symmetric positive semidefinite matrix.pdf:pdf},
issn = {0024-3795},
