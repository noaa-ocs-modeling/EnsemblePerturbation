keywords = {Northeast United States,extratropical cyclones,hurricanes,storm surge},
number = {9},
publisher = {IOP Publishing},
title = {{Comparing hurricane and extratropical storm surge for the Mid-Atlantic and Northeast Coast of the United States for 1979-2013}},
volume = {11},
year = {2016}
}
