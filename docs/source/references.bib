pages = {1--21},
publisher = {Elsevier Ltd},
title = {{Two-way nesting in split-explicit ocean models: Algorithms, implementation and validation}},
url = {http://dx.doi.org/10.1016/j.ocemod.2012.03.003},
volume = {49-50},
year = {2012}
}
