keywords = {Arrival time,COMCOT,Evacuation,Hikurangi subduction margin,Inundation modelling,Local tsunami,New Zealand,Structural damage,leapfrog,tsunami},
mendeley-tags = {New Zealand,leapfrog,tsunami},
month = {aug},
number = {1},
pages = {415--445},
title = {{Tsunami inundation in Napier, New Zealand, due to local earthquake sources}},
