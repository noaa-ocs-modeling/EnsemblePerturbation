keywords = {Finite-volume and finite-element methods,Large-scale ocean circulation modeling,Unstructured meshes},
pages = {195--210},
publisher = {Elsevier Ltd},
title = {{Ocean modeling on unstructured meshes}},
url = {http://dx.doi.org/10.1016/j.ocemod.2013.05.005},
