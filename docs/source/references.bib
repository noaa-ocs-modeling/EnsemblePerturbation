keywords = {Barometers,Coastal ocean,Coastal zones,Computational fluid dynamics,Computer simulation,Convergence of numerical methods,Error analysis,Grid convergence,Mathematical models,Richardson based error estimations,Shallow water model,Storm surge,grid,hurricane,hurricanes,hydrodynamic equations,modeling,shallow water,storm,storm surges},
