isbn = {14635003},
issn = {14635003},
journal = {Ocean Modelling},
keywords = {Finite-volume,Global ocean model,MPAS-Ocean,Multi-resolution,Spherical Centroidal Voronoi Tesselations},
pages = {211--232},
